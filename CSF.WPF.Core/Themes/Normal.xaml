<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CSF.WPF.Core.Themes">
    <SolidColorBrush x:Key="NormalFontForeground" Color="#FF009000" />
    <SolidColorBrush x:Key="Background1" Color="#FFF0F0F0" />

    <SolidColorBrush x:Key="DataFontForeground" Color="Aqua" />

    <Style x:Key="CataGridColumnHeaderStyle" TargetType="DataGridColumnHeader">
        <Setter Property="Foreground" Value="{DynamicResource NormalFontForeground}" />
        <Setter Property="Height" Value="24" />
    </Style>
</ResourceDictionary>